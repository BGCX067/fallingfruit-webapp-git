<p>
  <b>Lat:</b>
  <%= @location.lat %>
</p>

<p>
  <b>Lng:</b>
  <%= @location.lng %>
</p>

<p>
  <b>Author:</b>
  <%= @location.author %>
</p>

<p>
  <b>Title:</b>
  <%= @location.title %>
</p>

<p>
  <b>Description:</b>
  <%= @location.description %>
</p>

<p>
  <b>Season start:</b>
  <%= @location.season_start %>
</p>

<p>
  <b>Season stop:</b>
  <%= @location.season_stop %>
</p>

<p>
  <b>Region:</b>
  <%= @location.region.nil? ? "?" : @location.region.name %>
</p>

<p>
  <b>Type:</b>
  <%= @location.type.nil? ? "?" : @location.type.name %>
</p>

<p>
  <b>Address:</b>
  <%= @location.address %>
</p>
